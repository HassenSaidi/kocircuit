import "github.com/kocircuit/kocircuit/lib/strings"

Equal(string?) {
	return: strings.GoEqualStrings(string)
}

Len(string?) {
	return: strings.GoLenStrings(string)
}

// Join concatenates a sequence of strings with a given delimiter inbetween.
// string is a sequence of strings. delimiter is an optional string, equal to the empty string by default.
Join(string?, delimiter) {
	return: strings.GoJoinStrings(string: string, delimiter: delimiter)
}

Tree(delimiter, prefix, middle, suffix) {
	return: strings.GoStringTree(
		delimiter: delimiter
		prefix: prefix
		middle: middle
		suffix: suffix
	)
}

Flush(trees?) {
	return: strings.GoFlush(trees)
}

Quote(string?) {
	return: strings.GoQuote(string)
}
