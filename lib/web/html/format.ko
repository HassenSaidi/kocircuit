import "github.com/kocircuit/kocircuit/lib/strings"

FormDIV(f?, a) {
	return: DIV(Form(f: f, a: a))
}

FormP(f?, a) {
	return: P(Form(f: f, a: a))
}

Form(f?, a) {
	return: Format(
		format: f
		args: a
		withString: formatString(string?) {
			return: Yield(
				if: Not(strings.Equal(string, ""))
				then: T(string)
			)
		}
		withArg: Return
	)
}
