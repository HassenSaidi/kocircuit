import "github.com/kocircuit/kocircuit/lib/strings"

FmtDIV(f?, a) {
	return: DIV(Fmt(f: f, a: a))
}

FmtP(f?, a) {
	return: P(Fmt(f: f, a: a))
}

Fmt(f?, a) {
	return: Format(
		format: f
		args: a
		withString: formatString(string?) {
			return: Yield(
				if: Not(strings.Equal(string, ""))
				then: T(string)
			)
		}
		withArg: Return
	)
}
