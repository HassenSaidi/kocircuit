
import "github.com/aljabrio/aljabrio/web/telegram" // to access go implementations

Dial(token, offset, pollNano) {
	return: telegram.GoDialTelegram(token: token, offset: offset, pollNano: pollNano)
}

Accept(on?) {
	return: telegram.GoAcceptTelegram(on: on)
}

Reply(to, with) {
	return: telegram.GoReplyTelegram(to: to, with: with)
}
